[user]
	name = michael guldan
	emaill = michaelguldan@gmail.com
	email = michaelguldan@gmail.com
[core]
	autocrlf = input
    safecrlf = true
	pager = less -r

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  ol = log --pretty=format:"%h%x09%an%x09%ad%x09%s"
  type = cat-file -t
  dump = cat-file -p

[push]
	default = matching
[filter "lfs"]
	smudge = git-lfs smudge %f
	required = true
	clean = git-lfs clean %f
