# SSH Config
Host *
    ForwardAgent no
    ForwardX11 no
    ForwardX11Trusted yes
    Protocol 2
    ServerAliveInterval 60
    ServerAliveCountMax 30
    UseRoaming no 

# allow cached keys in Mac High Sierra
Host *
   IgnoreUnknown UseKeychain,AddKeysToAgent
   AddKeysToAgent yes
   UseKeychain yes

#  The usual suspects
Host tc
    HostName tilde.club
    User michaelcoyote
    #LocalForward 119 127.0.0.1:119

Host rarchive
    Hostname 174.129.171.128
    User ubuntu
    IdentityFile ~/.ssh/graphite.pem

